if(bronze1.getTotalCost() == silver1.getTotalCost() && bronze1.getTotalCost() == gold1.getTotalCost())
		System.out.println("Bronze, Silver and Gold packages are the same price");
			else if(bronze1.getTotalCost() == silver1.getTotalCost() && bronze1.getTotalCost() < gold1.getTotalCost())
			System.out.println("Bronze and Silver are the cheapest");
				else if(bronze1.getTotalCost() == gold1.getTotalCost() && bronze1.getTotalCost() < silver1.getTotalCost())
					System.out.println("Bronze and Gold are the cheapest");
						else if(silver1.getTotalCost() == gold1.getTotalCost() && silver1.getTotalCost() < bronze1.getTotalCost())
						System.out.println("Silver and Gold are the cheapest");
							else if(bronze1.getTotalCost() < silver1.getTotalCost() && bronze1.getTotalCost() < gold1.getTotalCost())
							System.out.println("Bronze package is the cheapest");
								else if(silver1.getTotalCost() < gold1.getTotalCost())
								System.out.println("Silver package is the cheapest");
									else 
										System.out.println("Gold package is the cheapest");
